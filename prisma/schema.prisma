generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model User {
  id           String   @id @default(uuid())
  createdAt    DateTime @default(now())
  updatedAt    DateTime @updatedAt
  email        String   @unique
  authProvider String
  providerId   String?
  firstName    String
  lastName     String?
  isOnboarded  Boolean  @default(false)
  imageUrl     String?
}
